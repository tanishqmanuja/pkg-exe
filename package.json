{
  "name": "@tanishqmanuja/pkg-exe",
  "version": "1.0.1",
  "description": "A tool for making resedit-able exe using @vercel/pkg",
  "main": "bin/index.js",
  "bin": {
    "pkg-exe": "bin/index.js",
    "@tanishqmanuja/pkg-exe": "bin/index.js"
  },
  "scripts": {
    "start": "node bin/index.js",
    "start:dev": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "build": "tsc -p ."
  },
  "keywords": [
    "pkg",
    "resedit",
    "exe"
  ],
  "author": "tanishqmanuja",
  "license": "ISC",
  "dependencies": {
    "pkg": "^5.7.0",
    "resedit": "^1.6.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/yargs": "^17.0.10",
    "nodemon": "^2.0.18",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}